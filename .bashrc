export PS1='\[\033[0;33m\]\W\[\033[0m\] # '

alias run='source /root/.bashrc'

# start edit bash, uses for editing bashrc on IOS
alias SEB='mv /root/.bashrc /root/bashrc.txt'

# end edit bash, uses for stop editing bashrc on IOS
alias EEB='mv /root/bashrc.txt /root/.bashrc '

alias la='ls -a'
alias ..='cd ..'
